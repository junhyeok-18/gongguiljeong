{{>layout/admin-header}}

<table style="border: 1px solid black;">
    <tr>
        <th>공구일정 등록한 사람</th>
        <td>
            <input type="text" id="scheduleRegisteredPerson" value="{{userCode}}" readonly>
        </td>
    </tr>
    <tr>
        <th>공구일정 카테고리</th>
        <td>
            <input type="text" id="scheduleCategory">
        </td>
    </tr>
    <tr>
        <th>공구일정 브랜드</th>
        <td>
            <input type="text" id="scheduleBrand">
        </td>
    </tr>
    <tr>
        <th>공구일정 인플루언서</th>
        <td>
            <input type="text" id="scheduleInfluencer">
        </td>
    </tr>
    <tr>
        <th>공구일정 시작날짜</th>
        <td>
            <input type="text" id="scheduleStartDate" maxlength="12">
        </td>
    </tr>
    <tr>
        <th>공구일정 종료날짜</th>
        <td>
            <input type="text" id="scheduleEndDate" maxlength="12">
        </td>
    </tr>
    <tr>
        <td>
            <button type="button" id="btn-cancel" value="schedule">취소</button>
        </td>
        <td>
            <button type="button" id="btn-save" value="schedule">등록</button>
        </td>
    </tr>
</table>

{{>layout/admin-footer}}