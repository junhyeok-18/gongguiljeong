{{/exception}}
{{^exception}}
    <p>exception : 없음</p>
{{/exception}}
{{#message}}
    <p>message : {{message}}</p>
{{/message}}
{{^message}}
    <p>message : 없음</p>
{{/message}}
<p>path : {{path}}</p>
</body>
</html>