{{>layout/admin-header}}

<table style="border: 1px solid black;">
    <tr>
        <th>브랜드 번호</th>
        <td>
            <input type="text" id="brandCode" value="{{brand.brandCode}}" readonly>
        </td>
    </tr>
    <tr>
        <th>브랜드 이름(한글)</th>
        <td>
            <input type="text" id="brandNameKr" value="{{brand.brandNameKr}}">
        </td>
    </tr>
    <tr>
        <th>브랜드 이름(영어)</th>
        <td>
            <input type="text" id="brandNameEng" value="{{brand.brandNameEng}}">
        </td>
    </tr>
    <tr>
        <th>브랜드 색상</th>
        <td>
            <input type="text" id="brandColor" value="{{brand.brandColor}}">
        </td>
    </tr>
    <tr>
        <th>브랜드 소개글</th>
        <td>
            <input type="text" id="brandIntroduction" value="{{brand.brandIntroduction}}">
        </td>
    </tr>
    <tr>
        <th>브랜드 링크</th>
        <td>
            <input type="text" id="brandLink" value="{{brand.brandLink}}">
        </td>
    </tr>
    <tr>
        <td>
            <button type="button" id="btn-cancel" value="brand">취소</button>
        </td>
        <td>
            <button type="button" id="btn-update" value="brand">수정</button>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            <button type="button" id="btn-delete" value="brand">삭제</button>
        </td>
    </tr>
</table>

{{>layout/admin-footer}}